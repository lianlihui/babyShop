@import "../../../wxParse/wxParse.wxss";
.product{
  width: 100%;
  height: 100%;
  background: #f4f4f4;
}
.product-header{
  width:100%;
  background: #ffffff;
}
.product-img{
  width: 100%;
  height: 650rpx;
}
.swiper-image{
  height: 100%;
}
.slide-image{
  width: 100%;
  height: 620rpx;
}

.product-title{
  padding: 0rpx 27rpx;
}
.product-title-txt{
  padding-right: 175rpx;
  line-height: 1.3;
  color: #414141;
  font-size: 28rpx;
  margin: 25rpx 0rpx;
  display: block;
  position: relative;
}
.product-title-txt-xz{
  position: absolute;
  display: block;
  top: 0rpx;
  right: 30rpx;
  color: #7b7b7b;
  font-size: 30rpx;
}
.product-title-txt-img{
  position: absolute;
  display: block;
  top: 0rpx;
  right: 0rpx;
  width: 24rpx;
  height: 24rpx;
}
.product-zj{
  width: 100%;
  margin-bottom: 20rpx;
}
.product-zj-item{
  width: 25%;
  display: inline-block;
}
.product-zj-item-btn{
  width:150rpx;
  line-height:42rpx;
  height:42rpx;
  font-size:28rpx;
  display:block;
  border-radius:100rpx;
}
.product-zj-item-btn.qx{
  background:#f2506d;
  color:#ffffff;
}
.product-zj-item-btn.jc{
  background:#ff9dac;
  color:#ffffff;
}
.product-zj-item-btn.bc{
  background:#dddddd;
  color:#7b7b7b;
}
.product-zj-item-btn.qc{
  background:#dddddd;
  color:#7b7b7b;
}

.product-title-price{
  font-size:28rpx;
  color:#585858;
  margin-bottom: 25rpx;
  display:inline-block;
  text-align: left;
  width: 33%;
}
.product-price-red{
  color: #ff4965;
  font-size: 28rpx;
}
.product-title-price-del{
  font-size:28rpx;
  color:#585858;
  display:inline-block;
  position:relative;
  width: 33%;
  text-align: left;
}
.product-info{
  padding: 0rpx 27rpx;
  margin-top:30rpx;
  background: #ffffff;
}
.product-info-detail{
  margin-top:30rpx;
  background: #ffffff;
}
.product-info-tag{
  height: 100rpx;
  line-height: 100rpx;
  color: #343333;
  font-size: 36rpx;
}
.product-info-tag-detail{
  padding: 0rpx 27rpx;
  height: 100rpx;
  line-height: 100rpx;
  color: #343333;
  font-size: 36rpx;
}
.product-image {
  width: 345rpx;
  height: 345rpx;
  margin: 0 auto;
}
.product-info-spec{
  margin-bottom: 30rpx;
  font-size:30rpx;
  position: relative;
}
.desc-color{
  padding: 0 5px;
  color:#414141;
}
.item-tag{
  display: inline-block;
  color: #7b7b7b;
}
.item-txt{
  display: inline-block;
  color: #414141;
  position: absolute;
  left: 220rpx;
  top:0rpx;
}

.product-enjoy{
  margin-top:30rpx;
  background: #ffffff;
  margin-bottom: 120rpx;
  overflow: hidden;
}
.product-enjoy-tag{
  padding: 0rpx 27rpx;
  height: 100rpx;
  line-height: 100rpx;
  color: #343333;
  font-size: 36rpx;
  border-bottom: 1px solid #ebebeb;
}

.product-list {
  overflow: hidden;
  margin-right: -1px;
}
.product-list-item {
  width: 50%;
  float: left;
  padding: 23rpx 15rpx;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  border-right: 1px solid #ebebeb;
  border-bottom: 1px solid #ebebeb;
}
.product-desc {
  font-size: 28rpx;
  color: #414141;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space:nowrap;
}

.del-price {
  font-size: 28rpx;
  color: #ff4965;
  display:inline-block;
}
.del-price-yj {
  font-size: 24rpx;
  color: #585858;
  display: inline-block;
  margin-left: 20rpx;
  position: relative;
}

.order-list-item-dzf-foot{
  height: 110rpx;
  position: fixed;
  bottom: 0rpx;
  width: 100%;
  right: 0rpx;
  border-top: 1rpx #ebebeb solid;
  z-index:1;
  background: #ffffff;
  overflow:hidden;
}
.btn-cart{
  width:220rpx;
  line-height: 110rpx;
  height: 110rpx;
  background: #ff9dac;
  font-size: 30rpx;
  color: #ffffff;
  display: block;
  float: right;
  border-radius: 0rpx;
}
.btn-buy{
  width:220rpx;
  line-height: 110rpx;
  height: 110rpx;
  background: #ff4965;
  font-size: 30rpx;
  color: #ffffff;
  display: block;
  float: right;
  border-radius: 0rpx;
}
.order-list-item-dzf-foot .item-edit{
  display: inline-block;
  vertical-align: middle;
  width: 57rpx;
  height: 53rpx;
  margin-left: 27rpx;
  position: relative;
  overflow: visible;
}
.order-list-item-dzf-foot .item-edit-img{
  width: 57rpx;
  height: 53rpx;
}
.item-edit-hh{
  display: inline-block;
  vertical-align: middle;
  width: 97rpx;
  height:110rpx;
  margin-left: 40rpx;
  background: #fff;
  border-radius: 0;
  border: 0;
  padding: 0;
}
.item-edit-hh:after {
  border: 0;
}
.item-edit-img-hh{
  width: 50rpx;
  height: 50rpx;
  margin-top:27rpx;
}
.item-edit-sc{
  display: inline-block;
  vertical-align: middle;
  width: 76rpx;
  height:110rpx;
  margin-left: 10rpx;
}
.item-edit-img-sc{
  width: 46rpx;
  height: 46rpx;
  margin-top:30rpx;
}
.order-list-item-dzf-foot .item-edit-nums{
  padding: 0 8rpx;
  height: 30rpx;
  line-height: 30rpx;
  background: #ff4965;
  position: absolute;
  top: 0rpx;
  right: -20rpx;
  color: #fff;
  font-size:28rpx;
  border-radius: 20rpx;
}

/**弹框样式*/
.modal-close {
  width: 62rpx;
  height: 62rpx;
  position: absolute;
  top: -31rpx;
  right: -31rpx;
  z-index: 10;
}
.modal-close-img {
  width: 51rpx;
  height: 51rpxrpx;
  margin-top: 5rpx;
  margin-left: 5rpx;
}
.modal-wrap {
  position: fixed;
  overflow: visible;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 1000;
  -webkit-overflow-scrolling: touch;
  outline: 0;
  background: rgba(0,0,0,0.2);
}
.modal {
  width: 615rpx;
  background: #fff;
  border-radius: 10rpx;
  position: relative;
  overflow: visible;
}
.modal-header{
  padding: 0rpx 30rpx;
}
.modal-header-item{
  border-bottom:1rpx #ebebeb solid;
  font-size:36rpx;
  color:#343333;
  height:100rpx;
  line-height: 100rpx;
}
.modal-body {
  padding: 0rpx 30rpx;
  height:600rpx;
  overflow: auto;
}
.modal-item{
  padding: 20rpx 0;
  border-bottom: 1rpx #ebebeb solid;
  font-size: 28rpx;
  color: #414141;
  position: relative;
}
.modal-item-desc{
  width: 160rpx;
  display: inline-block;
  vertical-align: middle;
}
.modal-item-inp{
  display: inline-block;
  vertical-align: middle;
  color: #ff4965;
}
.modal-item-inp .inp{
  display: inline-block;
  vertical-align: middle;
  border: 1rpx solid #ebebeb;
  width: 100rpx;
  text-align: center;
}
.modal-item-inp .ocon{
  display: inline-block;
  vertical-align: middle;
  border: 1rpx solid #ebebeb;
  width: 50rpx;
  height:1.4rem;
  line-height: 1.4rem;
  text-align: center;
}
.modal-item-inp .ocon-l {
  border-right: none;
}
.modal-item-inp .ocon-r {
  border-left: none;
}
.modal-spec-item{
  height: 70rpx;
  line-height: 70rpx;
  font-size: 28rpx;
  color: #414141;
}
.spec-wrap{
  border-bottom:1rpx #ebebeb solid;
  padding:20rpx 0;
}
.modal-body .spec {
  display: inline-block;
  height: 45rpx;
  line-height: 45rpx;
  font-size: 24rpx;
  color: #777;
  padding: 0 18rpx;
  box-sizing: border-box;
  border: 1rpx solid #7b7b7b;
  border-radius: 15rpx;
  margin-right: 38rpx;
  margin-bottom: 10rpx;
}
.modal-body .spec.active {
  background: #ff4965;
  color: #ffffff;
  border: 1rpx solid #ff4965;
}

.modal-footer{
  margin-top:30rpx;
  padding: 0 30rpx;
  margin-bottom:50rpx;
}
.modal-qd{
  width: 100%;
  line-height: 80rpx;
  height: 80rpx;
  background: #ff4965;
  font-size: 30rpx;
  color: #ffffff;
  display: block;
  border-radius: 10rpx;
}
.modal-qx{
  width:400rpx;
  line-height: 80rpx;
  height: 80rpx;
  background: #ff9dac;
  font-size: 30rpx;
  color: #ffffff;
  display: block;
  border-radius: 100rpx;
  margin-top:20rpx;
}

.vertical-center {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
}

.comment-title {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 34rpx;
  color: #000;
  text-indent: 27rpx;
}
.comment-wrap{
  margin-top:30rpx;
}
.comment-box {
  background: #fff;
}
.comment-tab-box {
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  border-bottom: 1px solid #d6d6d6;
  font-size: 32rpx;
  overflow: visible;
}
.comment-tab {
  width: 33%;
  height: 100rpx;
  line-height: 100rpx;
  display: inline-block;
  vertical-align: middle;
  border-bottom: 1px solid #d6d6d6;
  position: relative;
  top: -1px;
}
.comment-tab.active {
  border-bottom: 1px solid #ff4965;
}
.comment-item {
  padding: 28rpx;
  border-bottom: 1px solid #d6d6d6;
}
.comment-item-top{
  position: relative;
}
.comment-pic {
  width: 60rpx;
  height: 60rpx;
  border-radius: 100%;
  display: inline-block;
  vertical-align: middle;
  margin-right: 20rpx;
}
.comment-name {
  font-size: 28rpx;
  color: #333;
}
.comment-time{
  font-size: 26rpx;
  color: #888;
  position: absolute;
  right: 0rpx;
  top:8rpx;
}
.comment-item-ct {
  margin: 20rpx 0;
  font-size: 26rpx;
  color: #888;
}
.upload-img {
  width: 150rpx;
  height: 150rpx;
  border-radius: 10rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 20rpx;
}
.comment-item-info {
  margin: 20rpx 0;
}
.comment-text {
  margin-right: 20rpx;
  font-size: 26rpx;
  color: #888;
}
.comment-item-nodata{
  height: 60rpx;
  line-height: 60rpx;
  width: 100%;
  text-align: center;
  background: #ffffff;
  color: #585858;
  font-size: 28rpx;
}