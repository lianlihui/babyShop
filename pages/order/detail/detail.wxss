}

.order-list-item-dzf-bd{
  padding:30rpx 27rpx;
  background:#fff;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  display: -webkit-box;
  border-top: 1rpx #ebebeb solid;
  position: relative;
}
.order-list-item-dzf-bd .item-left{
  width:160rpx;
  height:160rpx;
  margin-right:30rpx;
