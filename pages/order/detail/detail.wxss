  -webkit-box-flex: 1.0;
}
.order-list-item-dzf-bd .item-title{
  line-height: 1.3;
  color: #414141;
  font-size: 28rpx;
  margin-bottom: 25rpx;
  display: block;
}

.order-list-item-dzf-bd .item-day{
  color: #7b7b7b;
  font-size: 28rpx;
  margin-bottom: 5rpx;
  display: block;
}
.order-list-item-dzf-bd .item-price{
  color: #f2506d;
  font-size: 36rpx;
  display: block;
}

.order-list-item-foot{
  border-top: 1rpx #ebebeb solid;
  padding: 0rpx 27rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  position: relative;
  background: #ffffff;
}
.order-list-item-foot .left-tag{
  color: #7b7b7b;
  display: inline-block;
}
.order-list-item-foot .right-tag{
  color: #7b7b7b;
