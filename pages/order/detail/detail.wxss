}
.btn-qx{
  color: #7b7b7b;
  border: 1px #7b7b7b solid;
  margin-right: 45rpx;
}
.btn-zf{
  color: #f2506d;
  border: 1px #f2506d solid;
}

.order-list-item-dzf-index{
  height: 100rpx;
  line-height: 100rpx;
  position: fixed;
  bottom: 0rpx;
  width: 100%;
  right: 0rpx;
  border-top: 1rpx #ebebeb solid;
  z-index:1;
  background: #fe9eac;
  text-align: center;
  color: #ffffff;
  font-size: 36rpx;
}
/**弹框样式*/
.modal-close {
  width: 62rpx;
  height: 62rpx;
  position: absolute;
  top: -31rpx;
  right: -31rpx;
  z-index: 10;
}
.modal-close-img {
  width: 51rpx;
  height: 51rpxrpx;
  margin-top: 5rpx;
  margin-left: 5rpx;
}
.modal-wrap {
  position: fixed;
  overflow: visible;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 1000;
  -webkit-overflow-scrolling: touch;
  outline: 0;
  background: rgba(0,0,0,0.2);
}
.modal {
  width: 515rpx;
  background: #fff;
  border-radius: 10rpx;
  position: relative;
  overflow: visible;
}
.modal-header{
  padding: 0rpx 30rpx;
}
.modal-header-item{
  border-bottom:1rpx #ebebeb solid;
  font-size:36rpx;
  color:#343333;
  height:100rpx;
  line-height: 100rpx;
}
.modal-body-xz{
  padding: 0rpx 30rpx;
  overflow: auto;
}
.modal-body {
  padding: 0rpx 30rpx;
  height:230rpx;
  overflow: auto;
}
.modal-item{
  padding: 20rpx 0;
  border-bottom: 1rpx #ebebeb solid;
  font-size: 28rpx;
  color: #414141;
  position: relative;
}
.modal-item-desc{
  width: 160rpx;
  display: inline-block;
  vertical-align: middle;
}
.modal-item-inp{
  display: inline-block;
  vertical-align: middle;
  color: #ff4965;
}
.modal-item-inp .inp{
  display: inline-block;
  vertical-align: middle;
  border: 1rpx solid #ebebeb;
  width: 100rpx;
  text-align: center;
}
.modal-item-inp .ocon{
  display: inline-block;
  vertical-align: middle;
  border: 1rpx solid #ebebeb;
  width: 50rpx;
  height:1.4rem;
  line-height: 1.4rem;
  text-align: center;
}
.modal-item-inp .ocon-l {
  border-right: none;
}
.modal-item-inp .ocon-r {
  border-left: none;
}
.modal-spec-item{
  height: 70rpx;
  line-height: 70rpx;
  font-size: 28rpx;
  color: #414141;
}
.spec-wrap{
  border-bottom:1rpx #ebebeb solid;
  padding:20rpx 0;
}
.modal-body .spec {
  display: inline-block;
  height: 45rpx;
  line-height: 45rpx;
  font-size: 24rpx;
  color: #777;
  padding: 0 18rpx;
  box-sizing: border-box;
  border: 1rpx solid #7b7b7b;
  border-radius: 15rpx;
  margin-right: 38rpx;
  margin-bottom: 10rpx;
}
.modal-body .spec.active {
  background: #ff4965;
  color: #ffffff;
  border:none;
}

.modal-footer{
  margin-top:30rpx;
  padding: 0 30rpx;
  margin-bottom:50rpx;
}
.modal-qd{
  width: 100%;
  line-height: 80rpx;
  height: 80rpx;
  background: #ff4965;
  font-size: 30rpx;
  color: #ffffff;
  display: block;
  border-radius: 10rpx;
}
.modal-qx{
  width:300rpx;
  line-height: 80rpx;
  height: 80rpx;
  background: #ff9dac;
  font-size: 30rpx;
  color: #ffffff;
  display: block;
  border-radius: 100rpx;
  margin-top:20rpx;
}

.vertical-center {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
}
