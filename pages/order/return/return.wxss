.order-list{
  width: 100%;
  height: 100%;
  background: #f4f4f4;
}
/**添加按钮样式**/
.address-btn{
  width: 100%;
  border-bottom: 1rpx #ebebeb solid;
  padding:25rpx 0rpx;
  background: #ffffff;
}
.address-btn-impl{
  width:586rpx;
  height: 90rpx;
  line-height: 90rpx;
  background: #ffffff;
  font-size: 42rpx;
  color: #ff4965;
  border: 1rpx #ff4965 solid;
  border-radius: 100rpx;
}
.address-btn-add-icon{
  width: 21rpx;
  height: 19rpx;
  margin-top: -10rpx;
  display: inline-block;
  vertical-align: middle;
  font-size: 30rpx;
}
.address-btn-impl span {
  margin-right: 10rpx;
}
/**地址样式表**/
.address-wrap{
  width: 100%;
  position: relative;
  padding: 20rpx 25rpx;
  background:#fff;
  -webkit-box-sizing:border-box;
  box-sizing:border-box;
  display:-webkit-box;
}
.address-left{
  width:34rpx;
  height:75rpx;
  margin-right:30rpx;
  overflow:hidden;
  background:#fff;
}
.address-icon{
  width: 34rpx;
  height: 46rpx;
  margin-top: 30rpx;
  display: inline-block;
  vertical-align: middle;
}
.address-right{
  position: relative;
  -webkit-box-flex: 1.0;
}
.address-accept{
  font-size: 36rpx;
  color: #414141;
  margin-bottom: 10rpx;
  position: relative;
}
.address-name{
  display: inline-block;
}
.address-phone{
  display: inline-block;
  position: absolute;
  right: 0rpx;
}
.address-info{
  display: inline-block;
  line-height: 1.3;
  font-size: 28rpx;
  color: #414141;
}
/**======待支付样式======**/
.order-list-item-dzf{
  background: #ffffff;
  margin: 30rpx 0rpx;
}
.order-list-item-dzf-head{
  padding: 0rpx 27rpx;
  font-size: 36rpx;
  position: relative;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1rpx #ebebeb solid;
}
.order-list-item-dzf-head .orderno-tag{
  color: #7b7b7b;
  display: inline-block;
}
.order-list-item-dzf-head .orderno-imp{
  color: #7b7b7b;
  display: inline-block;
  margin-left:20rpx; 
}
.order-list-item-dzf-bd{
  padding:40rpx 27rpx 0rpx;
  background:#fff;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  display: -webkit-box;
  border-bottom: 1rpx #ebebeb solid;
  position: relative;
}
.order-list-item-dzf-bd .item-edit{
  width: 40rpx;
  height: 41rpx;
  position: absolute;
  top: 40rpx;
  right: 27rpx;
}
.order-list-item-dzf-bd .item-left{
  width:215rpx;
  height:215rpx;
  margin-right:20rpx;
  overflow:hidden;
  background:#fff;
}
.order-list-item-dzf-bd .item-img{
  width:215rpx;
  height:215rpx;
}
.order-list-item-dzf-bd .item-right{
  position: relative;
  -webkit-box-flex: 1.0;
}
.order-list-item-dzf-bd .item-title{
  padding-right: 44rpx;
  line-height: 1.3;
  color: #414141;
  font-size: 28rpx;
  margin-bottom: 20rpx;
  display: block;
}
.order-list-item-dzf-bd .item-day{
  color: #7b7b7b;
  font-size: 28rpx;
  margin-bottom: 20rpx;
  display: block;
}
.item-price-red{
  color: #ff4965 !important;
}

.orderno-icon{
  width: 32rpx;
  height: 60rpx;
  background: url(data:image/png;base64,) no-repeat right center;
  background-size: 30rpx;
  position: absolute;
  top: 51%;
  margin-top: -30rpx;
}
.orderno-icon.active {
  background: url(data:image/png;base64,) no-repeat right center;
  background-size: 30rpx;
}
.item-noborderbottom{
  border-bottom:none !important;
}
.order-list-item-dzf-head .orderno-jf{
  font-size: 28rpx;
  color: #414141;
  display: inline-block;
  margin-left: 45rpx;
}
.order-list-item-dzf-head .orderno-jfmoney{
  font-size: 28rpx;
  color: #ff4965;
  display: block;
  float: right;
}
.order-remark{
  width:100%;
  padding-top:20rpx;
  background: #ffffff;
}
.order-remark-textarea{
  height: 172rpx;
  font-size: 24rpx;
  color: #333;
  width:100%;
  margin-top:2rpx;
  border: 1rpx #d6d6d6 solid;
  padding:10rpx;
}

.order-list-item-dzf-foot{
  height: 110rpx;
  line-height: 110rpx;
  position: fixed;
  bottom: 0rpx;
  width: 100%;
  right: 0rpx;
  border-top: 1rpx #ebebeb solid;
  z-index:1;
  background: #ffffff;
}
.foot-hj{
  display: inline-block;
  margin-left: 27rpx;
  font-size: 36rpx;
  color: #414141;
}
.foot-je{
  display: inline-block;
  margin-left: 18rpx;
  font-size: 36rpx;
  color: #ff4965;
}
.btn-qx{
  width:290rpx;
  line-height: 110rpx;
  height: 110rpx;
  background: #ff4965;
  font-size: 42rpx;
  color: #ffffff;
  display: block;
  float: right;
  border-radius: 0rpx;
}
.order-wl{
  width: 100%;
  background: #ffffff;
  padding: 0rpx 27rpx;
  margin-top: 44rpx;
  position: relative;
}
.order-wl .yj{
  display: block;
  height: 78rpx;
  line-height: 78rpx;
  text-align: center;
  font-size: 30rpx;
  color: #ffffff;
  background: #ff9dac;
  width:206rpx;
}
.order-wl .no{
  display: block;
  height: 78rpx;
  line-height: 78rpx;
  font-size: 30rpx;
  color: #858585;
  background: #f6f6f6;
  position: absolute;
  left: 233rpx;
  right: 0rpx;
  top:0rpx;
  padding-left: 70rpx;
  border: 1rpx #ebebeb solid;
}

.order-zwl{
  width: 100%;
  background: #ffffff;
  padding-top:10rpx;
  margin-bottom: 111rpx;
  position: relative;
}
.order-zwl .zyj{
  display: block;
  height: 78rpx;
  line-height: 78rpx;
  text-align: center;
  font-size: 30rpx;
  color: #ffffff;
  background: #ff4965;
  width:206rpx;
}
.order-zwl .zno{
  display: block;
  height: 78rpx;
  line-height: 78rpx;
  font-size: 30rpx;
  width: 100%;
  border: 1rpx #ebebeb solid;
}
.order-wl-tab{
  width: 100%;
  background: #ffffff;
  padding-top:22px;
  height:78rpx;
}
.order-wl-item{
  width: 50%;
  float: left;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  height:78rpx;
  line-height:78rpx;
  background-color: #f6f6f6;
  text-align: center;
  color: #858585;
  font-size: 30rpx;
}
.order-wl-item.active{
  width: 50%;
  float: left;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  height:78rpx;
  line-height:78rpx;
  background-color: #ff9dac;
  text-align: center;
  color: #ffffff;
  font-size: 30rpx;
}