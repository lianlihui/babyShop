/**index.wxss**/
.container {
  padding-bottom: 20rpx;
}
.page-top {
  width: 100%;
  height: 140rpx;
  position: relative;
  background: #ff4663;
  color: #fff;
  border-bottom: 30rpx solid #f4f4f4;
}
.page-top-bg {
  width: 100%;
  height: 260rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}
.page-top-lf,
.page-top-rt {
  margin-top: 19rpx;
  display: inline-block;
  vertical-align: middle;
  color: #fff;
}
.page-top-lf {
  width: 102rpx;
  height: 102rpx;
  margin-left: 25rpx;
  margin-right: 20rpx;
}
.page-top-img {
  width: 102rpx;
  height: 102rpx;
  border-radius: 10rpx;
}
.page-top-name {
  font-size: 36rpx;
  margin-bottom: 16rpx;
  line-height: 1;
  color: #fff;
}
.page-top-num {
  font-size: 28rpx;
  color: #fff;
}
.section-title {
  margin-left: 25rpx;
  height: 94rpx;
  line-height: 94rpx;
  color: #282828;
  font-size: 36rpx;
  border-bottom: 1px solid #ebebeb;
}
.section-box {
  width: 100%;
  display: -webkit-box;
  padding: 30rpx 0 30rpx 25rpx;
  border-bottom: 30rpx solid #f4f4f4;
  font-size: 28rpx;
  color: #7b7b7b;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.section-lf {
  width: 128rpx;
  height: 128rpx;
  overflow: hidden;
}
.section-img {
  width: 128rpx;
}
.section-ct,
.section-rt {
  -webkit-box-flex: 1.0;
          box-flex: 1.0;
}
.section-ct {
  width: 360rpx;
  padding: 0 25rpx;
}
.section-rt {
  padding-top: 10rpx;
  padding-right: 25rpx;
  text-align: right;
}
.section-price {
  color: #414141;
  line-height: 1.5;
}
.section-name {
  margin-top: 10rpx;
  line-height: 1.5;
  margin-bottom: 20rpx;
}

.section-list{
  padding-top:30rpx;
}
.section-item{
  display: -webkit-box;
  padding: 0rpx 25rpx;
  position: relative;
}
.section-time{
  width: 160rpx;
  height: 170rpx;
  overflow: hidden;
  border-right: 2rpx solid #dcdcdc;
  text-align: center;
}
.section-time.last{
  border-right: none;
}
.section-time-day{
  font-size: 34rpx;
  color: #7b7b7b;
}
.section-time-time{
  font-size: 28rpx;
  color: #7b7b7b;
}
.section-time-day.active{
  color: #f2506d;
}
.section-time-time.active{
  color: #f2506d;
}
.section-address {
  -webkit-box-flex: 1.0;
          box-flex: 1.0;
}
.section-address-info{
  font-size: 28rpx;
  color: #7b7b7b;
  padding-left: 25rpx;
}
.section-address-info.active{
  color: #f2506d;
}
.section-address-phone{
  font-size: 30rpx;
  color: #f7ac0a;
  padding-left: 25rpx;
  margin-top:20rpx; 
}
.orderno-icon{
  width: 25rpx;
  height: 25rpx;
  background: url(data:image/png;base64,) no-repeat right center;
  background-size: 25rpx;
  position: absolute;
  top:5rpx;
  left:172.5rpx;
}
.orderno-icon.active{
  width: 25rpx;
  height: 25rpx;
  background: url(data:image/png;base64,) no-repeat right center;
  background-size: 25rpx;
  position: absolute;
  top:5rpx;
  left:172.5rpx;
}