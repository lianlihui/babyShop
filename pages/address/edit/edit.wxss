.address{
  width: 100%;
  height: 100%;
}

.addrss-edit{
  width:100%;
  margin-top: 20rpx;
  background-color: #ffffff;
}

.addrss-edit-item{
  width:100%;
  padding-left:27rpx;
  -webkit-box-sizing:border-box;
  box-sizing:border-box;
}

.addrss-edit-item-impl{
  border-bottom: 1rpx solid #f0f0f0;
  padding: 32rpx 0;
  position: relative;
  -webkit-box-sizing:border-box;
  box-sizing:border-box;
}

.addrss-edit-item-text{
  font-size: 36rpx;
  color: #4a4a4a;
  position:absolute;
  top:50%;
  margin-top: -21rpx;
  left: 0;
}
.addrss-edit-item-inp{
  font-size: 36rpx;
  color: #333;
  margin-left:190rpx;
  /*height: 28rpx;
  line-height: 28rpx;*/
}

.addrss-edit-item-area{
  font-size: 36rpx;
  color: #333;
  margin-left:185rpx;
  height: 28rpx;
  line-height: 28rpx;
}

.address-edit-item-picker{
  display:inline-block;
  margin-left:150rpx;
  font-size: 24rpx;
  color: #333;
}
.addrss-edit-item-xtext{
  font-size: 36rpx;
  color: #333333;
  position:absolute;
  top:30rpx;
  left: 0;
}
.addrss-edit-item-textarea{
  height: 20rpx;
  padding: 0;
  font-size: 36rpx;
  color: #333;
  margin-left:190rpx;
  width:548rpx;
  margin-top:2rpx;
}
.addrss-edit-item-implm{
  border-bottom: 1rpx solid #f0f0f0;
  padding: 32rpx 0;
  position: relative;
  -webkit-box-sizing:border-box;
  box-sizing:border-box;
}
.addrss-edit-item-mtext{
  font-size: 36rpx;
  color: #333333;
  margin-left:35rpx;
}
.addrss-edit-item-icon {
  width: 32rpx;
  height: 60rpx;
  background: url(data:image/png;base64,) no-repeat right center;
  background-size: 30rpx;
  position: absolute;
  top: 51%;
  margin-top: -30rpx;
  left: 0;
}
.addrss-edit-item-icon.active {
  background: url(data:image/png;base64,) no-repeat right center;
  background-size: 30rpx;
}

.address-add-btn{
  width: 100%;
  margin-top:70rpx;
  background:#fff;
}
.address-add-impl,
.addrss-edit-delete {
  width:586rpx;
  height: 90rpx;
  line-height: 90rpx;
  background: #ff4965;
  font-size: 42rpx;
  color: #ffffff;
  border-radius: 100rpx;
  margin-bottom: 20rpx;
}

/*选择器*/
.picker-view-wrap {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0,0,0,.6);
  z-index: 1000;
}
.picker-view {
  width: 100%; 
  height: 400rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  background: #fff;
}
.picker-view-header{
  width: 100%; 
  height: 80rpx;
  position: relative;
  background-color: #fff;
  padding: 0 35rpx;
  -webkit-box-sizing:border-box;
  box-sizing:border-box;
  border-bottom: 1px solid #d5d5d5;
}
.picker-view-header-qd{
  width:100rpx;
  height:80rpx;
  line-height:80rpx;
  font-size: 30rpx;
  color:#66d3fc;
  text-align:center;
  position:absolute;
  top:50%;
  margin-top:-40rpx;
  right:35rpx;
}
.picker-view-header-qx{
  width:100rpx;
  height:80rpx;
  line-height:80rpx;
  font-size:30rpx;
  color:#333;
  text-align:center;
  position:absolute;
  top:50%;
  margin-top:-40rpx;
  left:35rpx;

}
.picker-view-impl{
  width: 100%; 
  height: 320rpx;
}
.addr-item {
  text-align: center;
  font-size: 30rpx;
}