.order-header{
  width:100%;
  height: 100rpx;
  background-color: #f9f9f9;
  border-bottom: 2rpx solid #ebebeb;
}

.order-header-item{
  width: 20%;
  height: 100rpx;
  line-height: 100rpx;
  display: inline-block;
  text-align: center;
  color: #333333;
  font-size: 30rpx;
}
.span-item-active{
  height: 100rpx;
  display: inline-block;
  border-bottom: 2rpx solid #f2506d;
}
.span-item{
  height: 100rpx;
  display: inline-block;
}
.order-list{
  width: 100%;
  height: 100%;
  background: #f4f4f4;
}
/**======待支付样式======**/
.order-list-item-dzf{
  background: #ffffff;
  margin-bottom: 30rpx;
  padding-top: 30rpx;
}
.order-list-item-dzf-head{
  padding: 0rpx 27rpx;
  font-size: 36rpx;
  position: relative;
}
.order-list-item-dzf-head .orderno-tag{
  color: #7b7b7b;
  display: inline-block;
}
.order-list-item-dzf-head .orderno-imp{
  color: #7b7b7b;
  display: inline-block;
  margin-left:30rpx; 
}
.order-list-item-dzf-head .money{
  color: #ff4965;
  display: inline-block;
  position: absolute;
  right: 27rpx;
}
.order-list-item-dzf-bd{
  padding:0rpx 27rpx 12rpx;
  background:#fff;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  display: -webkit-box;
  border-bottom: 1rpx #ebebeb solid;
  position: relative;
}
.order-list-item-dzf-bd .item-edit{
  width: 40rpx;
  height: 41rpx;
  position: absolute;
  top: 40rpx;
  right: 27rpx;
}
.order-list-item-dzf-bd .item-left{
  width:160rpx;
  height:160rpx;
  margin-right:30rpx;
  overflow:hidden;
  background:#fff;
}
.order-list-item-dzf-bd .item-img{
  width:160rpx;
  height:160rpx;
}
.order-list-item-dzf-bd .item-right{
  position: relative;
  -webkit-box-flex: 1.0;
  box-flex: 1.0;
}
.order-list-item-dzf-bd .item-title{
  padding-right: 95rpx;
  line-height: 1.3;
  color: #414141;
  font-size: 28rpx;
  margin-bottom: 25rpx;
  display: block;
}
.order-list-item-dzf-bd .dfh-item-title{
  line-height: 1.3;
  color: #414141;
  font-size: 28rpx;
  margin-bottom: 20rpx;
  display: block;
}

.order-list-item-dzf-bd .item-day{
  color: #7b7b7b;
  font-size: 28rpx;
  margin-bottom: 25rpx;
  display: block;
}
.order-list-item-dzf-bd .item-spec{
  color: #7b7b7b;
  font-size: 28rpx;
  margin-bottom: 20rpx;
  display: block;
}
.order-list-item-dzf-bd .item-spec-one{
  width: 310rpx;
  display: inline-block;
}
.order-list-item-dzf-money{
  height: 92rpx;
  line-height: 92rpx;
  padding: 0rpx 27rpx;
  border-bottom: 1rpx #ebebeb solid;
  position: relative;
  font-size: 28rpx;
  color: #7b7b7b;
}
.order-list-item-dzf-money .item-num{
  display: block;
  float: right;
  margin-right: 32rpx;
}
.order-list-item-dzf-money .item-hj{
  display: block;
  float: right;
  margin-right: 2rpx;
}
.order-list-item-dzf-money .item-price{
  display: block;
  float: right;
  font-size: 36rpx;
  color: #414141;
}
.order-list-item-dzf-money .dfh-item-price{
  display: block;
  float: right;
  font-size: 36rpx;
  color: #414141;
  margin-right: 32rpx;
}

.all-btn-com{
  margin-top:28rpx;
  width:210rpx;
  line-height: 60rpx;
  height: 60rpx;
  background: #ffffff;
  font-size: 30rpx;
  display: block;
  float: right;
  border-radius: 100rpx;
}
.order-list-item-all-foot{
  height: 128rpx;
  padding:0rpx 27rpx;
}
.btn-qx{
  color: #7b7b7b;
  border: 1px #7b7b7b solid;
  margin-right: 45rpx;
}
.btn-zf{
  color: #f2506d;
  border: 1px #f2506d solid;
}
.btn-txfh{
  color: #7b7b7b;
  border: 1px #7b7b7b solid;
}

.order-list-item-dzf-foot{
  height: 110rpx;
  padding:0rpx 27rpx;
  position: fixed;
  bottom: 0rpx;
  width: 100%;
  right: 0rpx;
  border-top: 1rpx #ebebeb solid;
  z-index:1;
  background: #ffffff;
}

/**待支付模块特殊处理*/
